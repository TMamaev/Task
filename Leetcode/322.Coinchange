int coinChange(int* coins, int coinsSize, int amount) {
    int min = 999999;

    for (int i = 0; i < coinsSize; i++)
    {
        for (int j = 0; j < coinsSize; j++)
        {
            if ((amount % coins[i]) % coins[j] == 0 && amount / coins[i] + amount % coins[i] / coins[j] < min)
                min = amount / coins[i] + amount % coins[i] / coins[j];
        }
    }
    if (min < 999999) return min;
    return -1;
}