#include <cmath>
#include <iostream>

using namespace std;

int main()
{
    int a, b;
    cin >> a >> b;
    double gip = sqrt(a * a + b * b);
    cout << gip;

    return 0;
}




#include <cmath>
#include <iostream>

using namespace std;

int main()
{
    int a;
    cin >> a;
    cout << "The next number for the number "<<a<<" is "<<a+1;
    cout << "The previous number for the number "<<a<<" is "<<a-1;
    return 0;
}




#include <iostream>

using namespace std;

int main()
{
    int n, k;
    cin >> n >> k;
    cout << k / n;

    return 0;
}




#include <iostream>

using namespace std;

int main()
{
    int n, k;
    cin >> n >> k;
    int pol = k / n;
    cout<< k - pol * n;

    return 0;
}



#include <iostream>

using namespace std;

int main()
{
    int v,t;
    cin >> v >> t;
    if (v * t > 0)
        cout << (v * t) % 109;
    if (v * t < 0)
        cout << (109 + (v * t)) % 109;
    return 0;
}




#include <iostream>

using namespace std;

int main()
{
    int n;
    cin >> n;
    int k = 0; // счетчик
    for (int n1 = n; n1 != 0; n1 /= 10)
        k+=1;
    for (; k != 0; k--)
        n %= 10;
    cout << n;

    return 0;
}




#include <iostream>

using namespace std;

int main()
{
    int n;
    cin >> n;
    int n1 = n;
    n1 %= 10;
    cout << (n - n1) / 10;

    return 0;
}




#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n, result = 0;
    cin >> n;
    int n1 = n;
    while (n1 > 10) 
        n1 %= 10;
    n -= n1;
    int n2 = n;
    int k = 0;
    for (int n3 = n; n3 != 0; n3 /= 10)
        k += 1;
    while (n != pow(10, k-1)){
        result += 1;
        n -= 10;
    }
    cout << result;

    return 0;
}




#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n, ans1 = 0;
    cin >> n;
    int ans2 = n;
    while (ans2 > 10) 
        ans2 %= 10;
    n -= ans2;
    int n2 = n;
    int k = 0;
    for (int n3 = n; n3 != 0; n3 /= 10)
        k += 1;
    while (n != pow(10, k-1)){
        ans1 += 1;
        n -= 10;
    }
    int ans3;
    while (n != 0){
        ans3 ++;
        n -= 100;
    }
    cout << ans3 + ans1 + ans2;

    return 0;
}





#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n;
    cin >> n;
    int n1 = n;
    n1 %= 2;
    cout << n + n1 + ((n1 + 2) % 3);
    return 0;
}





#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n, k = 0;
    cin >> n;
    n %= 1440;
    for (; n > 60;)
    {
        k ++;
        n -= 60;
    }
    cout << k << " " << n;
}







#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n, h = 0, m = 0, s = 0;
    cin >> n;
    if (n > 86400)
        n %= 86400;
    if (n > 3600)
        for (; n > 3600;)
        {
            h ++;
            n -= 3600;
        }
    if (n > 60)
        for (; n > 60;)
        {
            m ++;
            n -= 60;
        }
    s = n;
    cout << h << ":";
    if ( m < 10)
        cout<<"0"<<m<<":";
    else 
        cout<<m<<":";

    if (s<10)
        cout<<"0"<<s;
    else 
        cout<<s;
    return 0;
}




#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int a, b, c;
    cin >> a >> b;
    c = a;
    a = b;
    b = c;
    cout << a << " " << b;
    return 0;
}





